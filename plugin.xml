<?xml version='1.0' encoding='UTF-8'?>
<plugin id='com.vulume.cordova.wfcamera' version='0.0.3' xmlns='http://apache.org/cordova/ns/plugins/1.0' xmlns:android='http://schemas.android.com/apk/res/android'>
  <name>
     dbcamera 
  </name>
  <description>
     Plugman compatible wrapper for DBCamera. 
  </description>
  <author>
      
  </author>
  <keywords>
     camera, ios 
  </keywords>
  <license>
     MIT 
  </license>
  <js-module name='dbcamera' src='www/dbcamera.js'>
    <clobbers target='cordova.plugins.dbcamera'/>
  </js-module>
  <platform name='ios'>
    <config-file parent='/*' target='config.xml'>
      <feature name='DBCamera'>
        <param name='ios-package' value='CDVdbcamera'/>
      </feature>
    </config-file>
    <source-file src='src/ios/DBCamera/CDVdbcamera.m'/>
    <framework src='Foundation.framework' autogen='true'/>
    <framework src='AVFoundation.framework' autogen='true'/>
    <framework src='CoreMedia.framework' autogen='true'/>
    <framework src='QuartzCore.framework' autogen='true'/>
    <header-file src='src/ios/DBCamera/Categories/UIImage+Asset.h' autogen='true'/>
    <header-file src='src/ios/DBCamera/Categories/UIImage+Bundle.h' autogen='true'/>
    <header-file src='src/ios/DBCamera/Categories/UIViewController+UIViewController_FullScreen.h' autogen='true'/>
    <header-file src='src/ios/DBCamera/Controllers/DBCameraContainerViewController.h' autogen='true'/>
    <header-file src='src/ios/DBCamera/Controllers/DBCameraViewController.h' autogen='true'/>
    <header-file src='src/ios/DBCamera/Headers/DBCameraDelegate.h' autogen='true'/>
    <header-file src='src/ios/DBCamera/Objects/DBCameraFilterCell.h' autogen='true'/>
    <header-file src='src/ios/DBCamera/Views/DBCameraFiltersView.h' autogen='true'/>
    <header-file src='src/ios/DBCamera/Views/DBCameraLoadingView.h' autogen='true'/>
    <header-file src='src/ios/DBCamera/Views/DBCameraView.h' autogen='true'/>
    <resource-file src='src/ios/DBCamera/Resources/DBCameraImages.xcassets' autogen='true'/>
    <resource-file src='src/ios/DBCamera/Localizations/DBCamera.bundle' autogen='true'/>
  </platform>
</plugin>